{% extends 'base.html.twig' %}

{% block title %}Station{% endblock %}

{% block body %}
    <h1>Station</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ station.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ station.name }}</td>
            </tr>
            <tr>
                <th>Lon</th>
                <td>{{ station.lon }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ station.lat }}</td>
            </tr>
            <tr>
                <th>Station_id</th>
                <td>{{ station.stationId }}</td>
            </tr>
            <tr>
                <th>Current_dockCount</th>
                <td>{{ station.currentDockCount }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('station_index') }}">back to list</a>

    <a href="{{ path('station_edit', {'id': station.id}) }}">edit</a>

    {{ include('station/_delete_form.html.twig') }}
{% endblock %}
